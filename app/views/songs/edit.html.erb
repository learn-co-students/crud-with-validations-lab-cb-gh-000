<h1>New Song Form</h1>

<%= form_for(@song) do |f| %>

  <label>Title</label>
  <%= f.text_field :title %>
  <br>
  <label>Artist Name</label>
  <%= f.text_field :artist_name %>
<br>
  <label>Was it released</label>
  <%= f.check_box :released %>
<br>
  <label>Release Year</label>
  <%= f.number_field :release_year %>
<br>
  <label>Genre</label>
  <%= f.text_field :genre %>
<br>
  <%= f.submit %>
<% end %>
